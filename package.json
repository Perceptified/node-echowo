{
  "name": "ts-echowo",
  "version": "0.7.1",
  "description": "Port of win-echowo to node / typescript",
  "main": "index.js",
  "scripts": {
    "clean": "pwsh ./build/buildScript.ps1 -mode clean",
    "cleanDebug": "pwsh ./build/buildScript.ps1 -mode 'clean debug'",
    "buildDebug": "pwsh ./build/buildScript.ps1 -mode 'build debug'",
    "runDebug": "pwsh ./build/buildScript.ps1 -mode 'run debug'",
    "cleanRelease": "pwsh ./build/buildScript.ps1 -mode 'clean release'",
    "buildRelease": "pwsh ./build/buildScript.ps1 -mode 'build release'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Perceptified/node-echowo.git"
  },
  "keywords": [
    "uwu",
    "owo"
  ],
  "author": "Markus Aigner",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/Perceptified/node-echowo/issues"
  },
  "homepage": "https://github.com/Perceptified/node-echowo#readme",
  "devDependencies": {
    "@types/node": "^20.11.25",
    "@types/tmi.js": "^1.8.6",
    "commander": "^10.0.1",
    "nexe": "^1.1.6",
    "tmi": "^0.2.2",
    "typescript": "^5.0.4"
  },
  "pkg": {
    "assets": [
      "build/Release/data/*.json"
    ]
  }
}
